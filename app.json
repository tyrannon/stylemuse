{
  "expo": {
    "name": "stylemuse-app",
    "slug": "stylemuse-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tyrannon.stylemuseapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "StyleMuse needs camera access to take photos of your clothing items for AI analysis",
        "NSPhotoLibraryUsageDescription": "StyleMuse needs photo library access to select clothing images for your virtual wardrobe",
        "NSLocationWhenInUseUsageDescription": "StyleMuse uses your location to provide weather-based outfit recommendations"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "058e32e8-871a-4b91-9660-862044af6d5c"
      }
    }
  }
}
